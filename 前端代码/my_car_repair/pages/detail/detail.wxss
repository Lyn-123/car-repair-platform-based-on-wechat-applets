.container {
  background-color: #f8f7f7;
}
.input-placeholder {
  background: #f2f4f6;
  border-radius: 10rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #909090;
}
.swiper {
  width: 100%;
  height: 589rpx;
}
.swiper swiper {
  width: 100%;
  height: 100%;
}
.swiper swiper swiper-item {
  width: 100%;
  height: 100%;
  position: relative;
}
.swiper swiper swiper-item .slide-image {
  width: 100%;
  height: 100%;
}
.info {
  padding: 42rpx 30rpx 34rpx;
  position: relative;
  background-color: #fff;
}
.info .Collection {
  position: absolute;
  top: 42rpx;
  right: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.info .Collection image {
  width: 38rpx;
  height: 35rpx;
}
.info .Collection view {
  margin-top: 10rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #232323;
}
.info .name {
  font-size: 44rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: #292929;
}
.info .title {
  margin-top: 42rpx;
  font-size: 36rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: #292929;
}
.info .number {
  margin-top: 33rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #a4a4a4;
}
.info .biaoqianList {
  margin-top: 28rpx;
  display: flex;
}
.info .biaoqianList .biaoqian {
  margin-right: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 108rpx;
  height: 32rpx;
  background: #e8fef9;
  border-radius: 2rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: #3cc7a2;
}
.info .biaoqianList .state1 {
  width: 108rpx;
  height: 32rpx;
  background: #e8eefe;
  border-radius: 2rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: #9bb6fb;
}
.info .biaoqianList .state2 {
  width: 108rpx;
  height: 32rpx;
  background: #ffece8;
  border-radius: 2rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: #ff8c71;
}
.info .Commission {
  display: flex;
  margin-top: 27rpx;
}
.info .Commission .p1 {
  font-size: 32rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: #656565;
}
.info .Commission .p2 {
  font-size: 32rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: #FF4C6F;
}
.info .money {
  margin-top: 27rpx;
  font-size: 36rpx;
  font-family: PingFang-SC-Regular;
  font-weight: bold;
  color: #ff4c6f;
}
.address {
  margin-top: 10rpx;
  width: 100%;
  padding: 25rpx 24rpx;
  position: relative;
  display: flex;
  align-items: center;
  background-color: #fff;
}
.address .dw {
  width: 28rpx;
  height: 32rpx;
}
.address view {
  margin-left: 11rpx;
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #404040;
}
.address .zy {
  width: 18rpx;
  height: 30rpx;
  position: absolute;
  right: 24rpx;
}
.detail {
  background-color: #fff;
  margin-top: 10rpx;
  padding: 36rpx 30rpx 25rpx;
}
.detail .item {
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #878787;
  margin-bottom: 34rpx;
}
.detail .word {
  width: 100%;
  border: 1rpx solid #3cc7a2;
  border-radius: 10rpx;
  padding: 15rpx;
}
.comment {
  margin-top: 17rpx;
  padding: 40rpx 15rpx 80rpx;
  background-color: #fff;
}
.comment .title {
  display: flex;
  align-items: center;
}
.comment .title image {
  width: 46rpx;
  height: 39rpx;
}
.comment .title view {
  margin-left: 10rpx;
  font-size: 34rpx;
  font-family: PingFang-SC-Medium;
  font-weight: bold;
  color: #242220;
}
.comment .content {
  margin-top: 66rpx;
}
.comment .content .item {
  margin-bottom: 66rpx;
}
.comment .content .item .itemTop {
  display: flex;
}
.comment .content .item .itemTop .itemTopLeft .avatar {
  border-radius: 50%;
  width: 80rpx;
  height: 80rpx;
}
.comment .content .item .itemTop .itemTopRight {
  padding: 5rpx 0;
  margin-left: 10rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.comment .content .item .itemTop .itemTopRight .tt {
  font-size: 30rpx;
  font-family: PingFang-SC-Medium;
  font-weight: bold;
  color: #171717;
}
.comment .content .item .itemTop .itemTopRight .tb {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #9c9d9d;
}
.comment .content .item .itemBottom {
  margin-left: 90rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: #0e0e0e;
}
.send {
  width: 100%;
  position: fixed;
  bottom: 0;
  background-color: #fff;
  padding: 20rpx 22rpx;
  display: flex;
  justify-content: space-between;
}
.send input {
  width: 538rpx;
  height: 70rpx;
  padding-left: 25rpx;
  color: #909090;
  background: #f2f4f6;
}
.send .btn {
  background: #39c7a1;
  border-radius: 10rpx;
  width: 156rpx;
  height: 70rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #ffffff;
  font-size: 30rpx;
}
.m-rating{
  margin-top:5rpx;
}
.m-rating image{
  width: 36rpx;
  height: 36rpx;
  float: left;
  position: relative;
  top: 6rpx;
  margin-right: 8rpx;
} 